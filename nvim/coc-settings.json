{
    "languageserver": {
        "ccls": {
            "command": "ccls",
            "filetypes": ["c", "cc", "cpp", "c++", "objc", "objcpp"],
            "rootPatterns": [".ccls", "compile_commands.json", ".git/", ".hg/"],
            "initializationOptions": {
                "cache": {
                    "directory": "/tmp/coc/"
                }
            }
        }
    },
    "jedi.executable.command": "~/.local/bin/jedi-language-server",
    "suggest.noselect": false
}
